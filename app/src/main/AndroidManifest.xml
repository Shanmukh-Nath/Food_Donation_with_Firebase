<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="foodonate">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the "MyLocation" functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AAHAARAPP"
        tools:ignore="AllowBackup">
        <activity android:name="com.example.foodonate.UserdataActivity"></activity>
        <activity android:name="com.example.foodonate.History" />
        <activity android:name="com.example.foodonate.MyPin" />
        <activity android:name="com.example.foodonate.About" />
        <activity android:name="com.example.foodonate.FoodMap" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBtAubRGSlTZanGLTPT3JrKWsRCFAXZzrE" />

        <activity android:name="com.example.foodonate.Receive" />
        <activity android:name="com.example.foodonate.Contact" />
        <activity android:name="com.example.foodonate.Signup" />
        <activity android:name="com.example.foodonate.Logup" />
        <activity android:name="com.example.foodonate.MainActivity" />
        <activity android:name="com.example.foodonate.Donate" />
        <activity android:name="com.example.foodonate.landingpage" />
        <activity
            android:name="com.example.foodonate.SplashScreen"
            android:theme="@style/SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>